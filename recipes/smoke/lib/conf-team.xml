<interfaces>
    <interface id="if-1" network="net" type="eth"/>
    <interface id="if-2" network="net" type="eth"/>
    <interface id="testiface" type="team">
        <options>
            <option name="teamd_config">
                {
                    "runner": {"name": "roundrobin"}
                }
            </option>
        </options>
        <slaves>
            <slave id="if-1"/>
            <slave id="if-2"/>
        </slaves>
        <addresses>
             <address value="{$testip}"/>
        </addresses>
    </interface>
</interfaces>
