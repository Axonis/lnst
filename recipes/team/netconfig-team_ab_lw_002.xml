<netconfig>
    <interface id="1" phys_id="1" type="eth">
        <options>
            <option name="teamd_port_config">
                {
		    "link_watch": {
			"name": "ethtool",
			"delay_up": 3000,
			"delay_down": 3000
		    }
                }
            </option>
        </options>
    </interface>
    <interface id="2" phys_id="2" type="eth">
        <options>
            <option name="teamd_port_config">
                {
		    "link_watch": {
			"name": "ethtool",
			"delay_up": 1000,
			"delay_down": 1000
		    }
                }
            </option>
        </options>
    </interface>
    <interface id="3" phys_id="3" type="eth">
        <options>
            <option name="teamd_port_config">
                {
		    "link_watch": {
			"name": "ethtool",
			"delay_up": 500,
			"delay_down": 500
		    }
                }
            </option>
        </options>
    </interface>
    <interface id="testiface" type="team">
        <options>
            <option name="teamd_config">
                {
                    "hwaddr": "00:11:22:33:44:55",
                    "runner": {"name": "activebackup"}
                }
            </option>
        </options>
        <slaves>
            <slave id="1"/>
            <slave id="2"/>
            <slave id="3"/>
        </slaves>
        <addresses>
             <address value="{$testip}"/>
             <address value="{$testip6}"/>
        </addresses>
    </interface>
</netconfig>
